{
  "name": "polyfill",
  "description": "A polyfill combinator",
  "version": "1.0.0",
  "author": {
    "name": "Jonathan Neal",
    "email": "jonathantneal+github@gmail.com",
    "url": "http://jonathantneal.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jonathantneal/polyfill"
  },
  "scripts": {
	"start": "node_modules/.bin/forever --minUptime 30000 --spinSleepTime 1000 app.js",
	"restart": "node_modules/.bin/forever restart app.js",
	"stop": "node_modules/.bin/forever stop app.js",
	"status": "node_modules/.bin/forever list | grep -q app.js"
  },
  "bugs": {
    "url": "https://github.com/jonathantneal/polyfill/issues"
  },
  "license": "CC0 1.0 Universal License",
  "dependencies": {
    "caniuse-db": "1.0.20140710",
    "express": "^4.4.4",
    "forever": "^0.11.1",
    "semver": "^2.3.1",
    "uglify-js": "^2.4.14",
    "useragent": "^2.0.8"
  }
}
